        "strict": true,
        "jsx": "react-jsx",
        "moduleResolution": "nodenext",
        "module": "nodenext",
        "paths": {
            "@/*": ["./*"]
        }
    },
    "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
